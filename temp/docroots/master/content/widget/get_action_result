<%args>
$this_url => $url
$panel => $context->panel->name
</%args>

<%perl>
my $result = $context->get_action_result($this_url, $panel);
if (defined $result) {
	$context->delete_action_result($this_url, $panel);
	return @{$result}{qw/action state args/};
} else {
	return (undef, undef, {});
}
</%perl>
